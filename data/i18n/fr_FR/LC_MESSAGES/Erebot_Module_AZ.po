msgid   "word.incorrect"
msgstr       "<b><var name=\"word\"/></b> n'existe pas ou n'est pas "
     "admissible pour ce jeu."#
msgid   "word.found"
msgstr       "<b>BINGO !</b> La réponse était effectivement <u><var name="
     "\"answer\"/></u>. Félicitations <b><var name=\"nick\"/></b> !"
#
msgid   "trigger.error"
msgstr  "Impossible d'enregistrer le déclencheur pour le jeu AZ"
#
msgid   "game.wordlists"
msgstr       "Les listes de mots suivantes sont disponibles : <for from="
     "\"lists\" item=\"list\"><b><var name=\"list\"/></b></for>."
#
msgid   "game.start"
msgstr       "Une nouvelle partie de <b>A-Z</b> commence sur <b><var name="
     "\"chan\"/></b> avec les listes suivantes : <for from=\"lists\" item="
     "\"list\"><b><var name=\"list\"/></b></for>."
#
msgid   "game.range"
msgstr  "Nouvel intervalle : <b><var name=\"min\"/></b> -- <b><var "
        "name=\"max\"/></b>"
#
msgid   "game.not-enough-words"
msgstr  "Il n'y a pas assez de mots dans les listes sélectionnées pour "
        "commencer une nouvelle partie."
#
msgid   "game.info"
msgstr       "<b>A-Z</b> (<for from=\"lists\" item=\"list\"><var name=\"list"
     "\"/></for>). Intervalle actuel : <b><var name=\"min\"/></b> -- "
     "<b><var name=\"max\"/></b> (<b><var name=\"attempts\"/></b> essais et "
     "<b><var name=\"bad\"/></b> mots incorrects)"
#
msgid   "game.epilog"
msgstr       "La réponse a été trouvée après <b><var name=\"attempts\"/></"
     "b> essais et <b><var name=\"bad\"/></b> mots incorrects."
#
msgid   "game.end"
msgstr       "La partie de <b>A-Z</b> a été arrêtée après <b><var name="
     "\"attempts\"/></b> essais et <b><var name=\"bad\"/></b> mots "
     "incorrects. La réponse était <u><var name=\"answer\"/></u>."
#
